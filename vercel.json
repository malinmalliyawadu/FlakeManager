{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "framework": null,
  "outputDirectory": "public",
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, s-maxage=31536000"
      },
      "dest": "/build/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
